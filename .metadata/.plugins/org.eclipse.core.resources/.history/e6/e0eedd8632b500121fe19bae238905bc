    <script>
    $(document).ready(function() {
      $('.bio').toggle();
      $('.describe').toggle();
      $('.prize').toggle();
      $('.finish').toggle();
      $('.giftcard').toggle();
    });
    </script>
    <script>
    $(".input-huge").keypress(function(event) {
      if (event.which == 13) {
        event.preventDefault();
        $('.search').toggle();
        $('.bio').toggle();
        $('.describe').toggle();
        $('.prize').toggle();
        $('.finish').toggle();
      }
    });
    </script>
